  "paths": {
    "/model/{id}/cancel": {
      "put": {
        "tags": [
          "model-controller"
        ],
        "summary": "Cancel a model run",
        "description": "Models that produce intermediate results, e.g. RANDIG or FSPro, will not do so.",
        "operationId": "cancelRun",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "responses": {
          "403": {
            "description": "You do not have permission to access the requested model",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDto"
                }
              }
            }
          },
          "400": {
            "description": "Model run was already in a terminal state",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDto"
                }
              }
            }
          },
          "401": {
            "description": "Missing or invalid credentials provided",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDto"
                }
              }
            }
          },
          "200": {
            "description": "Model was successfully cancelled",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ModelMetadataDto"
                }
              }
            }
          },
          "404": {
            "description": "Requested model does not exist",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDto"
                }
              }
            }
          },
          "500": {
            "description": "Internal Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDto"
                }
              }
            }
          }
        },
        "security": [
          {
            "basicAuth": []
          }
        ]
      }
    },
    "/resource": {
      "post": {
        "tags": [
          "resource-controller"
        ],
        "summary": "Upload a resource",
        "description": "Resources are auxiliary files that fire behavior models need in order to run, e.g. geotiff landscapes or barrier shapefiles",
        "operationId": "upload",
        "parameters": [
          {
            "name": "type",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "enum": [
                "GEOTIFF",
                "SHAPE"
              ]
            }
          }
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "required": ["file"],
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "400": {
            "description": "Uploaded resource failed initial validation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDto"
                }
              }
            }
          },
          "403": {
            "description": "You do not have permission to upload",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDto"
                }
              }
            }
          },
          "401": {
            "description": "Missing or invalid credentials provided",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDto"
                }
              }
            }
          },
          "200": {
            "description": "Resource upload succeeded",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResourceMetadataDto"
                }
              }
            }
          },
          "500": {
            "description": "Internal Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDto"
                }
              }
            }
          }
        },
        "security": [
          {
            "basicAuth": []
          }
        ]
      }
    },
    "/model": {
      "post": {
        "tags": [
          "model-controller"
        ],
        "summary": "Run a model",
        "description": "Resource IDs refer to previously-uploaded resources",
        "operationId": "uploadModel",
        "parameters": [
          {
            "name": "type",
            "in": "query",
            "description": "Which model type to run, valid values: FLAM, MTT_, RNDG, FSPR, FARS.",
            "required": true,
            "schema": {
              "type": "string",
              "enum": [
                "FLAM",
                "MTT_",
                "FARS",
                "RNDG",
                "FSPR"
              ]
            }
          },
          {
            "name": "landscape",
            "in": "query",
            "description": "Landscape ID. Enter ID returned from /resource endpoint.",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          },
          {
            "name": "ignition",
            "in": "query",
            "description": "Ignition ID. Enter ID returned from /resource endpoint.",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          },
          {
            "name": "barrier",
            "in": "query",
            "description": "Barrier ID. Enter ID returned from /resource endpoint.",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          },
          {
            "name": "griddedWindSpeed",
            "in": "query",
            "description": "Gridded Wind Speed ID. Enter ID returned from /resource endpoint. Only available for use with FLAM, MTT_ and RNDG.",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          },
          {
            "name": "griddedWindDir",
            "in": "query",
            "description": "Gridded Wind Direction ID. Enter ID returned from /resource endpoint. Only available for use with FLAM, MTT_ and RNDG.",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "encoding": {
                "file": {
                  "contentType": "application/octet-stream,application/zip,text/plain"
                }
              },
              "schema": {
                "type": "object",
                "required": ["file"],
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Model request has been successfully submitted",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ModelMetadataDto"
                }
              }
            }
          },
          "429": {
            "description": "You have exceeded your maximum concurrent runs for this model",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDto"
                }
              }
            }
          },
          "400": {
            "description": "Input file, landscape, or required resource failed validation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDto"
                }
              }
            }
          },
          "401": {
            "description": "Missing or invalid credentials provided",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDto"
                }
              }
            }
          },
          "403": {
            "description": "You do not have permission to run this model type",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDto"
                }
              }
            }
          },
          "500": {
            "description": "Internal Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDto"
                }
              }
            }
          }
        },
        "security": [
          {
            "basicAuth": []
          }
        ]
      }
    },
