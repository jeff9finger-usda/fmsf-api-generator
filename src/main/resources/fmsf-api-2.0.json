  "components": {
    "schemas": {
      "ErrorResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "error": {
            "type": "string"
          }
        }
      },
      "ModelMetadataDto": {
        "type": "object",
        "properties": {
          "runId": {
            "type": "integer",
            "format": "int64"
          },
          "owner": {
            "type": "string"
          },
          "modelType": {
            "type": "string",
