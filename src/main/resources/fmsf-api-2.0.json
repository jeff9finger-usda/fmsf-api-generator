{
  "openapi": "3.1.0",
  "info": {
    "title": "FMSF API",
    "description": "Fire Modeling Services Framework API",
    "version": "2.0.0"
  },
  "servers": [
    {
      "url": "https://fmsf2.firenet.gov/api",
      "description": "Generated server url"
    },
    {
      "url": "https://fmsf2-qa.firenet.gov/api",
      "description": "Generated server url"
    }
  ],
  "security": [
    {
      "basicAuth": []
    }
