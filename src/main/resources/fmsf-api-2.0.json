          },
          "folderName": {
            "type": "string"
          },
          "uploaded": {
            "type": "string",
            "format": "date"
          },
          "expiration": {
            "type": "string",
            "format": "date"
          },
          "projection": {
            "type": "string"
          },
          "resolution": {
            "type": "integer",
            "format": "int32"
          },
          "numRows": {
            "type": "integer",
            "format": "int32"
          },
          "numColumns": {
            "type": "integer",
            "format": "int32"
          },
          "numBands": {
            "type": "integer",
            "format": "int32"
          },
          "numCells": {
            "type": "integer",
            "format": "int32"
          },
