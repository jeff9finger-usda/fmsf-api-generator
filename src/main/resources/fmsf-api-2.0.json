          },
          "outputFolderSize": {
            "type": "integer",
            "format": "int64"
          },
          "landscapeId": {
            "type": "integer",
            "format": "int64"
          },
          "fuelbedId": {
            "type": "integer",
            "format": "int64"
          },
          "treeMapId": {
            "type": "integer",
            "format": "int64"
          },
          "statusMsg": {
            "type": "string"
          },
          "ignitionId": {
            "type": "integer",
            "format": "int64"
          },
          "barrierId": {
            "type": "integer",
            "format": "int64"
          },
          "limitingShapeId": {
            "type": "integer",
            "format": "int64"
          },
          "edgeHits": {
            "type": "integer",
            "format": "int32"
          },
          "proportionComplete": {
            "type": "number",
            "format": "float"
          },
          "firesFailed": {
            "type": "integer",
            "format": "int32"
          },
          "firesCompleted": {
            "type": "integer",
            "format": "int32"
          },
          "proportionBurned": {
            "type": "number",
            "format": "float"
          }
        }
      },
      "ResourceMetadataDto": {
